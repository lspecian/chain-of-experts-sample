{
  "name": "chain-of-experts",
  "version": "1.0.0",
  "description": "Chain of Experts LLM application with Langfuse monitoring",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn src/server.ts",
    "test": "jest",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down",
    "db:init": "ts-node src/vectordb/initDb.ts",
    "db:populate": "ts-node src/vectordb/populateDb.ts",
    "start:all": "npm run db:start && npm run db:init && npm run dev"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.804.0",
    "@google/generative-ai": "^0.24.1",
    "body-parser": "^1.20.2",
    "chromadb": "^2.4.3",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "langfuse": "^2.0.0",
    "openai": "^4.98.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.9.4",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  }
}
